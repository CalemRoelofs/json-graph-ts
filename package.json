{
  "name": "json-graph-ts",
  "version": "0.1.0",
  "description": "Json Graph implementation in TypeScript",
  "type": "module",
  "main": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "npm run lint && rm -rf dist/lib && tsc && tsc --build tsconfig.es5.json && webpack",
    "lint": "eslint . --ext .ts --fix",
    "test": "npm run compile && c8 ava",
    "prebuildpackage": "npm run test && rm dist/lib/es*/*.test* && cp docs/* dist/",
    "buildpackage": "cd dist && npm pack"
  },
  "keywords": [
    "jsongraph",
    "json-graph",
    "graph"
  ],
  "author": "Calem Roelofs",
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "ava": "^5.0.1",
    "c8": "^7.12.0",
    "eslint": "^8.27.0",
    "json-schema-to-typescript": "^11.0.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack-cli": "^5.0.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "webpack": "^5.75.0"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/lib/es6/"
      },
      "compile": false
    },
    "files": [
      "**/*.test.js"
    ]
  }
}